    "feed": "5.1.0",
    "joi": "18.0.1",
    "moment": "2.30.1",
    "pg": "8.16.3",
    "sql-ts": "7.1.0",
    "xml-formatter": "3.6.7",
    "xml2js": "0.6.2",
    "xmllint-wasm": "5.0.0"
  },
  "devDependencies": {
    "@hapi/code": "9.0.3",
    "@hapi/lab": "26.0.0",
    "aws-sdk-client-mock": "4.1.0",
    "proxyquire": "2.1.3",
